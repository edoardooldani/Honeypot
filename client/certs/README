DA honey/certs

openssl genrsa -out client-key.pem 2048

openssl req -new -key client-key.pem -out client.csr -config ../../CA/client.cnf

penssl x509 -req -in client.csr \
  -CA ../../CA/CA.pem -CAkey ../../CA/CA.key -CAcreateserial \
  -out client-cert.pem -days 365 -sha256 \
  -extfile ../../CA/client.cnf -extensions v3_req