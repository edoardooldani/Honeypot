[ req ]
default_bits       = 2048
prompt             = no
default_md         = sha256
x509_extensions    = v3_req
distinguished_name = dn

[ dn ]
C  = IT
ST = Italia
L  = Genoa
O  = Honeypot
OU = Server
CN = client.oldaedo@gmail.com
emailAddress = client.oldaedo@gmail.com

[ v3_req ]
subjectAltName = @alt_names
keyUsage = digitalSignature, keyEncipherment
extendedKeyUsage = clientAuth

[ alt_names ]
DNS.1 = raspberrypi.local
IP.1 = 192.168.1.68